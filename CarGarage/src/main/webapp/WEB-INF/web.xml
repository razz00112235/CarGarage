 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  			<servlet-name>Dispatcher</servlet-name>
  			<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>payment_data</servlet-name>
  	<display-name>payment_data</display-name>
  	<description></description>
  	<servlet-class>com.payment.payment_data</servlet-class>
  </servlet>
  <servlet-mapping>
  			<servlet-name>Dispatcher</servlet-name>
  			<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>payment_data</servlet-name>
  	<url-pattern>/payment_data</url-pattern>
  </servlet-mapping>
</web-app>
